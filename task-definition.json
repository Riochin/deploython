{
  "family": "deploython-task",
  "containerDefinitions": [
    {
      "name": "deploython-app",
      "image": "160885272337.dkr.ecr.ap-northeast-1.amazonaws.com/deploython-app:latest",
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "memory": 512,
      "cpu": 256,
      "essential": true
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::160885272337:role/ecsTaskExecutionRole"
}
